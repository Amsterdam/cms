{
    "name": "amsterdam/cms",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    },
    "require": {
        "composer/installers": "^1.9",
        "drupal/console": "^1.9.5",
        "drupal/consumers": "1.x-dev",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/devel_entity_updates": "3.x-dev",
        "drupal/devel": "4.x-dev",
        "drupal/elasticsearch_connector": "^7.0@alpha",
        "drupal/feeds": "3.x-dev",
        "drupal/imce": "2.x-dev",
        "drupal/jsonapi_extras": "3.x-dev",
        "drupal/paragraphs": "1.x-dev",
        "drupal/search_api": "1.x-dev",
        "drupal/typed_data": "1.x-dev",
        "drupal/uuid_extra": "2.x-dev",
        "drush/drush": "^10.3",
        "nodespark/des-connector": "7.x-dev",
        "webonyx/graphql-php": "v14.3.0"
    },
    "_require": {
        "drupal/consumer_image_styles": "3.x-dev",
        "drupal/backup_migrate": "5.0.x-dev",
        "drupal/config_update": "1.x-dev",
        "drupal/entity_count": "^1.0",
        "drupal/entity_reference_revisions": "1.x-dev",
        "drupal/entity_update": "2.0.x-dev",
        "drupal/upgrade_status": "^2.0",
        "drupal/rest_absolute_urls": "^1.0@beta"
    }
}
